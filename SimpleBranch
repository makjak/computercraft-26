args = {...}
length = args[1]
branches = args[2]

fuelState = 1

function getFuelState()
	turtle.select(16)
	if turtle.getItemCount()<1 then
		return 0
	elseif turtle.getFuelLevel()<20 then
		return 1
	else return 2
	end
end

function f()
	turtle.forward()
end

function df()
	turtle.dig()
	if not turtle.forward() then
		df()
	end
end

function turn()
	turtle.turnLeft()
	turtle.turnLeft()
end

function du()
	turtle.digUp()
	turtle.up()
end

function tunnel(length, boolean)
	local b = boolean
	for i=1, length do
		df()
		if not boolean and i%11==0 then
			turtle.select(15)
			turtle.placeDown()
			fuelState = getFuelState()
			if fuelState==0 then
				continue = false
				break
			elseif fuelState==1 then
				turtle.select(16)
				turtle.refuel(1)
			end
		elseif i%5==0 then
			fuelState = getFuelState()
			if fuelState==0 then
				continue = false
				break
			elseif fuelState==1 then
				turtle.select(16)
				turtle.refuel(1)
			end
		end
	end
	if b and continue then
		b = false
		du()
		turn()
		turtle.select(15)
		turtle.placeDown()
		tunnel(length, b)
	end
end

function emptyInventory(n)
	for i=1, n do
		turtle.select(i)
		turtle.drop()
	end
end

function seekChest()
	local blocksMoved = 0
	local isBlock, data = turtle.inspect()
	turtle.down()
	turtle.turnLeft()
	local found = false
	while not found do
		isBlock, data = turtle.inspect()
		if data.name=="minecraft:chest" then
			found = true
		else 
			f()
			blocksMoved = blocksMoved+1
			if blocksMoved%5==0 then
				fuelState = getFuelState()
				if fuelState==0 then
					continue = false
					break
				elseif fuelState==1 then
					turtle.select(16)
					turtle.refuel(1)
				end
			end
		end
	end
	if found then
		emptyInventory(14)
		turn()
		return blocksMoved
	end
end

function branching(lengthOfTunnels, numberOfBranches)
	continue = true
	for i=1, numberOfBranches do
		tunnel(lengthOfTunnels, true)
		if not continue then break end
		local back = seekChest()
		for i=1, back do
			f()
		end
		for i=1, 4 do
			df()
			turtle.digUp()
		end
		turtle.turnRight()
	end
end

branching(length, branches)